<?xml version="1.0"?>
<launch>
<node pkg="ndt_offline" type="bag_file_view" name="bag_file_view"  output="screen" >
<param name="odometry_topic" type="string" value="/odom" />
<param name="gt_topic" type="string" value="/state" />
<param name="t_merge_frames" type="int" value="1" />
</node>

<node pkg="rviz" type="rviz" name="rviz" args="-d $(find ndt_offline)/launch/show_bag.rviz" />

<arg name="path" default="$(find ndt_offline)/bag/arla_bags/" />
<arg name="file_1" default="Tapphall_Kyl1.bag" />
<node pkg="rosbag" type="play" name="player" output="screen" args="-r 20 $(arg path)$(arg file_1)"/>


</launch>

